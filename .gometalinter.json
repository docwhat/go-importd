{
  "Linters": {
    "gosec": {
      "Command": "gosec -fmt=csv -exclude=G107",
      "Pattern": "^(?P<path>.*?\\.go),(?P<line>\\d+),(?P<message>[^,]+,[^,]+,[^,]+)",
      "InstallFrom": "github.com/securego/gosec/cmd/gosec",
      "PartitionStrategy": "packages",
      "defaultEnabled": true,
      "IsFast": true
    }
  }
}
