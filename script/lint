#!/bin/bash
#
# Sets up all the dependencies, etc. so you can build and test.

set -euo pipefail
# shellcheck source=script/utilities.bash
source "$(dirname "$0")/utilities.bash"

header "Lint"
gometalinter \
  --deadline=5m \
  --vendored-linters \
  --vendor \
  --disable=gotype

# vim: set ft=sh :
